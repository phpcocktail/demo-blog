{
	"name": "phpcocktail/demo-blog",
	"description": "demo blog application for cocktail",
	"version": "1.1",
	"dist": {
		"url": "https://github.com/phpcocktail/demo-blog.git",
		"type": "vcs"
	},
	"repositories": [
		{
			"type": "package",
			"package": {
				"name": "phpcocktail/camarera",
				"version": "1.1",
				"source": {
					"url": "https://github.com/phpcocktail/camarera.git",
					"type": "git",
					"reference": "master"
				},
				"autoload": {
					"classmap": ["src/"],
					"files": ["bootstrap.php"]
				}
			}
		},
		{
			"type": "package",
			"package": {
				"name": "phpcocktail/cocktail",
				"version": "1.1",
				"source": {
					"url": "https://github.com/phpcocktail/cocktail.git",
					"type": "git",
					"reference": "master"
				},
				"autoload": {
					"classmap": ["src/"],
					"files": ["bootstrap.php"]
				}
			}
		}
	],
	"require": {
		"phpcocktail/camarera": "*",
		"phpcocktail/cocktail": "*"
	},
	"autoload": {
		"classmap": [
			"src/"
		],
		"files": [
			"bootstrap.php"
		]
	}
}
