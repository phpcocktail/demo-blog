{
	"name": "phpcocktail/demo-blog",
	"description": "demo blog application for cocktail",
	"version": "1.1",
	"dist": {
		"url": "https://github.com/phpcocktail/demo-blog.git",
		"type": "vcs"
	},
	"repositories": {
		"packagist": false,
		"phpcocktail/camarera": {
			"type": "package",
			"package": {
				"name": "phpcocktail/camarera",
				"version": "1.1",
				"source": {
					"url": "git@github.com:phpcocktail/camarera",
					"type": "git",
					"reference": "master"
				},
				"autoload": {
					"classmap": ["src/"],
					"files": ["bootstrap.php"]
				}
			}
		},
		"phpcocktail/cocktail": {
			"type": "package",
			"package": {
				"name": "phpcocktail/cocktail",
				"version": "1.1",
				"source": {
					"url": "git@github.com:phpcocktail/cocktail",
					"type": "git",
					"reference": "master"
				},
				"autoload": {
					"classmap": ["src/"],
					"files": ["bootstrap.php"]
				},
				"bin": ["shake.php"]
			}
		},
		"phpcocktail/block": {
			"type": "package",
			"package": {
				"name": "phpcocktail/block",
				"version": "1.1",
				"source": {
					"url": "git@github.com:phpcocktail/block",
					"type": "git",
					"reference": "master"
				},
				"autoload": {
					"classmap": ["src/"],
					"files": ["bootstrap.php"]
				}
			}
		}
	},
	"require": {
		"phpcocktail/camarera": "*",
		"phpcocktail/cocktail": "*",
		"phpcocktail/block": "*"
	},
	"autoload": {
		"classmap": ["src/"],
		"files": ["bootstrap.php"]
	}
}
